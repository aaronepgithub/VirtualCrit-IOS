{
  "name": "com.virtualcrit",
  "version": "2.0.0",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "displayName": "virtualcrit2",
  "dependencies": {
    "cordova-android": "^6.3.0",
    "cordova-custom-config": "^5.0.1",
    "cordova-ios": "^4.5.4",
    "cordova-plugin-ble-central": "^1.1.4",
    "cordova-plugin-compat": "^1.2.0",
    "cordova-plugin-device": "^1.1.7",
    "cordova-plugin-mauron85-background-geolocation": "^2.3.3"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-ble-central": {
        "BLUETOOTH_USAGE_DESCRIPTION": "REQUIRES BLE TO READ DATA"
      },
      "cordova-plugin-device": {},
      "cordova-custom-config": {},
      "cordova-plugin-mauron85-background-geolocation": {
        "GOOGLE_PLAY_SERVICES_VERSION": "+",
        "ICON": "@mipmap/icon",
        "SMALL_ICON": "@mipmap/icon",
        "ACCOUNT_NAME": "@string/app_name",
        "ACCOUNT_LABEL": "@string/app_name",
        "ACCOUNT_TYPE": "$PACKAGE_NAME.account",
        "CONTENT_AUTHORITY": "$PACKAGE_NAME",
        "ALWAYS_USAGE_DESCRIPTION": "This app always requires location tracking"
      }
    },
    "platforms": [
      "ios",
      "android"
    ]
  }
}